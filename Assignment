 	import java.util.scanner;
 	public class acad
 	{
 	public Static void main(String args[])
 	{
 	Scanner s=new Scanner(System.in);
 	int a=s.nextInt();
 	int b=s.nextInt();
 	int sum=a+b;
 	System.out.println(sum);
 	}
 	}


class acad{
 
public static void main(String args[])
 
{
 
Scanner s=new Scanner(System.in);
 
int a=s.nextInt();
 
int b=s.nextInt();
 
sum(a,b);
 
}}
 
////////////
 
public void sum(int a,int b)
 
{
 
int res=a+b;
 
System.out.println(res);
 
}
